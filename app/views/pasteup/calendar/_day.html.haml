- (Date.parse("#{@start}")..Date.parse("#{@end}")).each do |date|
  %tr
    %td
      = l(date, format: :middle, default: '')

    %td
      /戸田公園加工なし
      - if @blank_toda1_wip[date].present?
        = link_to "🐤#{@blank_toda1_wip[date]}件", wip_toda1_blank_index_path(date: date)
        \/ 計#{@blank_toda1_wip[date].to_i+@blank_toda1_done[date].to_i}件
        %br/

    %td
      /シルクスクリーンA🐭戸田公園第1
      - if @silkscreen_a_toda1_pasteup_wip[date].present?
        = link_to "🐤#{@silkscreen_a_toda1_pasteup_wip[date]}件", wip_pasteup_toda1_silkscreen_a_index_path(date: date)
        \/ 💭#{@silkscreen_a_toda1_wip[date].to_i}件
        %br/
        計#{@silkscreen_a_toda1[date].to_i}件

    %td
      /美女木 加工なし
      - if @blank_bijogi_wip[date].present?
        = link_to "🐤#{@blank_bijogi_wip[date]}件", wip_bijogi_blank_index_path(date: date)
        \/ 計#{@blank_bijogi_wip[date].to_i+@blank_bijogi_done[date].to_i}件

    %td
      /シルクスクリーンA美女木
      - if @silkscreen_a_bijogi_pasteup_wip[date].present?
        = link_to "🐤#{@silkscreen_a_bijogi_pasteup_wip[date]}件", wip_bijogi_silkscreen_a_index_path(date: date)
        \/ 計#{@silkscreen_a_bijogi_pasteup_wip[date]+@silkscreen_a_bijogi_done[date].to_i}件

    %td
      /インクジェット🐭戸田公園第1
      - if @inkjet_toda1_pasteup_wip[date].present?
        = link_to "🐤#{@inkjet_toda1_pasteup_wip[date]}件", wip_toda1_inkjet_index_path(date: date)
